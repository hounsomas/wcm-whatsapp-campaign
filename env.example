# Configuration du serveur
PORT=5000
NODE_ENV=development

# JWT Secret (changez ceci en production)
JWT_SECRET=wcm-secret-key-2024-change-in-production

# WhatsApp Business API (optionnel - pour l'intégration réelle)
WHATSAPP_TOKEN=your_whatsapp_business_token_here
WHATSAPP_PHONE_NUMBER_ID=your_phone_number_id_here

# Base de données
DATABASE_URL=sqlite:wcm.db

# Configuration des uploads
MAX_FILE_SIZE=16777216
UPLOAD_PATH=uploads/

# Rate limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100 